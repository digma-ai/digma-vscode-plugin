{
  	"name": "digma-for-python",
  	"displayName": "Digma Plugin for Python",
  	"description": "",
  	"version": "0.0.1",
	"publisher": "Digma",
	"icon": "icon.png",
  	"engines": {
  	  	"vscode": "^1.63.0"
  	},
	"repository": {
		"type": "git",
		"url": "https://github.com/digma-ai/digma-vscode-plugin-python"
    },
	"bugs": {
        "url": "https://github.com/digma-ai/digma-vscode-plugin-python/issues"
    },
	"homepage": "https://github.com/digma-ai/digma-vscode-plugin-python",
  	"categories": [
  	  	"Other"
  	],
  	"activationEvents": [
  	  	"onLanguage:python"
  	],
  	"main": "./out/extension.js",
  	"contributes": {
  	  	"commands": [
  	  	  	{
  	  	  	  	"title": "Enable CodeLens",
  	  	  	  	"command": "digma.enableCodeLens",
  	  	  	  	"category": "Digma"
  	  	  	},
  	  	  	{
  	  	  	  	"title": "Disable Codelens",
  	  	  	  	"command": "digma.disableCodeLens",
  	  	  	  	"category": "Digma"
  	  	  	}
  	  	],
  	  	"configuration": {
  	  	  	"properties": {
  	  	  	  	"digma.enableCodeLens": {
  	  	  	  	  	"type": "boolean",
  	  	  	  	  	"default": true
  	  	  	  	},
  	  	  	  	"digma.url": {
  	  	  	  	  	"type": "string",
  	  	  	  	  	"default": "http://127.0.0.1:5000"
  	  	  	  	}
  	  	  	}
  	  	}
  	},
  	"dependencies": {
  	  	"vscode-languageclient": "^7.0.0",
  	  	"node-fetch": "^2.6.6",
  	  	"pyright": "1.1.197"
  	},
  	"scripts": {
  	  	"vscode:prepublish": "npm run compile",
  	  	"compile": "tsc -p ./",
  	  	"watch": "tsc -watch -p ./",
  	  	"pretest": "npm run compile && npm run lint",
  	  	"lint": "eslint src --ext ts",
  	  	"test": "node ./out/test/runTest.js"
  	},
  	"devDependencies": {
  	  	"@types/glob": "^7.1.4",
  	  	"@types/mocha": "^9.0.0",
  	  	"@types/node": "14.x",
  	  	"@types/vscode": "^1.63.0",
		"@types/node-fetch": "^2.5.10",
  	  	"@typescript-eslint/eslint-plugin": "^5.1.0",
  	  	"@typescript-eslint/parser": "^5.1.0",
  	  	"@vscode/test-electron": "^1.6.2",
  	  	"eslint": "^8.1.0",
  	  	"glob": "^7.1.7",
  	  	"mocha": "^9.1.3",
  	  	"typescript": "^4.4.4"
  	}
}
