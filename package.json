{
  	"name": "digma",
  	"displayName": "Digma Plugin for VSCode",
  	"description": "",
  	"version": "0.0.1",
  	"publisher": "Digma",
  	"icon": "icon.png",
  	"engines": {
  	  	"vscode": "^1.63.0"
  	},
  	"repository": {
  	  	"type": "git",
  	  	"url": "https://github.com/digma-ai/digma-vscode-plugin"
  	},
  	"bugs": {
  	  	"url": "https://github.com/digma-ai/digma-vscode-plugin/issues"
  	},
  	"homepage": "https://github.com/digma-ai/digma-vscode-plugin",
  	"categories": [
  	  	"Other"
  	],
  	"activationEvents": [
  	  	"onStartupFinished"
  	],
  	"main": "./out/extension.js",
  	"contributes": {
  	  	"views": {
  	  	  	"digma": [  
  	  	  	  	{
  	  	  	  	  	"type": "webview",
  	  	  	  	  	"id": "context",
  	  	  	  	  	"name": "Context"
  	  	  	  	},	  
  	  	  	  	{
  	  	  	  	  	"type": "tree",
  	  	  	  	  	"id": "errorFlowList",
  	  	  	  	  	"name": "Error Flows List"
  	  	  	  	},
  	  	  	  	{
  	  	  	  	  	"type": "webview",
  	  	  	  	  	"id": "errorFlowDetails",
  	  	  	  	  	"name": "Error Flow Details"
  	  	  	  	}
  	  	  	]
  	  	},
  	  	"viewsContainers": {
  	  	  	"activitybar": [
  	  	  	  	{
  	  	  	  	  	"id": "digma",
  	  	  	  	  	"title": "Digma",
  	  	  	  	  	"icon": "icon.png"
  	  	  	  	}
  	  	  	]
  	  	},
  	  	"configuration": {
  	  	  	"properties": {
  	  	  	  	"digma.enableCodeLens": {
  	  	  	  	  	"type": "boolean",
  	  	  	  	  	"default": true
  	  	  	  	},
  	  	  	  	"digma.url": {
  	  	  	  	  	"type": "string",
  	  	  	  	  	"default": "https://localhost:7106"
  	  	  	  	},
  	  	  	  	"digma.environment": {
  	  	  	  	  	"type": "string"
  	  	  	  	},
  	  	  	  	"digma.hideFramesOutsideWorkspace": {
  	  	  	  	  	"type": "bool",
					"defult": true
  	  	  	  	},
  	  	  	  	"digma.sourceControl": {
  	  	  	  	  	"type": "string",
					"default": "None",
					"enum": ["None", "Git"]
  	  	  	  	}
  	  	  	}
  	  	}
  	},
  	"dependencies": {
  	  	"@vscode/webview-ui-toolkit": "^0.8.4",
		"@vscode/codicons": "0.0.20",
  	  	"https": "^1.0.0",
  	  	"moment": "^2.29.1",
  	  	"node-fetch": "^2.6.6",
  	  	"vscode-languageclient": "^7.0.0"
  	},
  	"scripts": {
  	  	"vscode:prepublish": "npm run compile",
  	  	"compile": "tsc -p ./",
  	  	"watch": "tsc -watch -p ./",
  	  	"pretest": "npm run compile && npm run lint",
  	  	"lint": "eslint src --ext ts",
  	  	"test": "node ./out/test/runTest.js"
  	},
  	"devDependencies": {
  	  	"@types/glob": "^7.1.4",
  	  	"@types/mocha": "^9.0.0",
  	  	"@types/node": "14.x",
  	  	"@types/node-fetch": "^2.5.10",
  	  	"@types/vscode": "^1.63.0",
  	  	"@typescript-eslint/eslint-plugin": "^5.1.0",
  	  	"@typescript-eslint/parser": "^5.1.0",
  	  	"@vscode/test-electron": "^1.6.2",
  	  	"eslint": "^8.1.0",
  	  	"glob": "^7.1.7",
  	  	"mocha": "^9.1.3",
  	  	"typescript": "^4.4.4"
  	}
}
